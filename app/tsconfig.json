{
  "compilerOptions": {
    "jsx": "react", // или "react-jsx" для новых версий React
    "module": "esnext",
    "target": "es6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src"]
}
